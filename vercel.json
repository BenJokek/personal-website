{
  "version": 2,
  "builds": [
    {
      "src": "src/api/backend.py",
      "use": "@vercel/python"
    },
    {
      "src": "src",
      "use": "@sveltejs/adapter-vercel"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/.*",
      "dest": "src/api/backend.py"
    }
  ]
}
